{
  "name": "plasticboy",
  "version": "2.2.0",
  "description": "3D Model Collection App for Almaty with Advanced Caching & Telegram Bot",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "bot": "node bot.js",
    "bot:dev": "nodemon bot.js",
    "both": "concurrently \"npm run dev\" \"npm run bot:dev\"",
    "both:prod": "concurrently \"npm start\" \"npm run bot\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node setup.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "node-telegram-bot-api": "^0.63.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "qr-code",
    "geolocation",
    "cache",
    "telegram",
    "telegram-bot",
    "oauth",
    "almaty",
    "collection",
    "gamification"
  ],
